{
  "compilerOptions": {
    "downlevelIteration": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "types/**/*.ts",
    ".next/types/**/*.ts"
, "src/app/api/service/route.js", "src/components/dashboard/professional/Service/addService/page.jsx", "src/app/api/service/serveur.js", "src/app/api/workPlace/route.js", "src/app/api/workPlace/get/route.js", "src/app/api/post/edit/[id]/route.js", "src/app/api/serviceAdditionnel/delete/[id]/route.js", "src/app/api/serviceAdditionnel/get/route.js"  ],
  "exclude": ["node_modules"]
}
